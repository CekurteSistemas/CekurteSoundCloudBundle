{
    "name": "cekurte/soundcloudbundle",
    "description": "The SoundCloud Bundle",
    "keywords": ["sound", "cloud", "soundcloud"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Jo√£o Paulo Cercal",
            "email": "sistemas@cekurte.com",
            "homepage": "http://sistemas.cekurte.com/"
        }
    ],
    "require": {
        "mptre/php-soundcloud": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mptre/php-soundcloud",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/mptre/php-soundcloud.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": { "Cekurte\\SoundCloudBundle": "" }
    },
    "target-dir": "Cekurte/SoundCloudBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}